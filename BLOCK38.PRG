User Function Block38()

SC5->( dbSetOrder( 1 ) )

If ! ( SC5->( dbSeek( xFilial( 'SC5' )+ M->ZA_PEDIDO ) ) )

   MsgInfo( 'Pedido de venda não cadastrado...' )

Else

   RecLock( 'SC5', .F.)
   SC5->C5_CANHOTO := M->ZA_CANHOTO
   SC5->( dbUnLock() )

End

Return( M->ZA_PEDIDO )