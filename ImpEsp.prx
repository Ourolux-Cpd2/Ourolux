Static Function Pnr010ImpBh(nSaldoAnt, nSaldoAtu, nCredito, nDebito)
Local aArea 	:= GetArea()
Local nValor 	:= 0
Local lRet		:= .F.

nSaldoAnt	:= 0
nDebito		:= 0
nCredito	:= 0
nSaldoAtu	:= 0

dbSelectArea( "SPI" )
dbSetOrder(2)
dbSeek( SRA->RA_FILIAL + SRA->RA_MAT )
While SPI->( !Eof() .and. PI_FILIAL+PI_MAT == SRA->( RA_FILIAL+RA_MAT ) )

	PosSP9(SPI->PI_PD,SRA->RA_FILIAL,"P9_TIPOCOD")

	// Totaliza Saldo Anterior
	If SPI->PI_DATA < dPerIni
		If SP9->P9_TIPOCOD $  "1*3"
			nValor := SPI->PI_QUANT
		   	If lSexagenal
				nSaldoAnt := __TimeSum(nSaldoAnt,nValor)  
			Else
				nSaldoAnt := nSaldoAnt + nValor 
			EndIf 
		Else
			nValor := SPI->PI_QUANT
			If lSexagenal
				nSaldoAnt := __TimeSub(nSaldoAnt,nValor)  
			Else
				nSaldoAnt := nSaldoAnt - nValor 
			EndIf
		EndIf
	ElseIf SPI->PI_DATA <= dPerFim
		If SP9->P9_TIPOCOD $  "1*3"
			nValor := SPI->PI_QUANT
		   	If lSexagenal
				nCredito := __TimeSum(nCredito,nValor)  
			Else
				nCredito := nCredito + nValor 
			EndIf 
		Else
			nValor := SPI->PI_QUANT
			If lSexagenal
				nDebito := __TimeSum(nDebito,nValor)  
			Else
				nDebito := nDebito + nValor 
			EndIf
		EndIf	
	Else
		Exit
	Endif

	dbSelectArea( "SPI" )
	dbSkip()

Enddo

If nSaldoAnt <> 0 .or. nCredito > 0 .or. nDebito > 0
	lRet := .T.
	nSaldoAtu := ( nSaldoAnt + nCredito ) - nDebito
EndIf

RestArea(aArea)

Return lRet

