Private nLin	   := 0
Private oEspelho   := NIL       

MakeDir( cLocal )

oEspelho := FWMSPrinter():New(SRA->RA_MAT, IMP_PDF, lAdjustToLegacy,cLocal, lDisableSetup, , , , , , .F., )
